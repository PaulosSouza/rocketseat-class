{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@screens": [
        "./src/screens"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@dtos/*": [
        "./src/dtos/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ]
    }
  }
}
